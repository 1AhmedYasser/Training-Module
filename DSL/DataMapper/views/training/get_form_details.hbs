{
    "name":"{{name}}",
    "response": "{{ details.formResponse }}",
    "required_slots": [
        {{#each details.requiredSlots }}
            {
                "slot_name": "{{ slot_name }}",
                "question": "{{ question }}"
            }{{#unless @last}},{{/unless}}
        {{/each}}
    ],
    "ignored_intents": [
        {{#each  details.ignoredIntents }}
            "{{this}}"{{#unless @last}},{{/unless}}
        {{/each}}
    ]
}